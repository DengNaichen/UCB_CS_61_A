   "source": [
    "def fib(n):\n",
    "    pred, curr = 0, 1\n",
    "    k = 1\n",
    "    while k < n:\n",
    "        pred, curr = curr, curr + pred\n",
    "        k += 1\n",
    "    return curr\n",
